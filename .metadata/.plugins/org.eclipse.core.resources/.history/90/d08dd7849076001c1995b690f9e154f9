package com;

public class q1 {
	public static void main(String[] args) {
		printfibonacci(20);
	}
	
	public static void printfibonacci(int num) {
		int a=0,b=1,c=1;
		double sum=0.0;
		for(int i=1;i<=num;i++) {
			System.out.print(c+" ");
			sum+=c;
			c=a+b;
			a=b;
			b=c;
		}
		System.out.println();
		System.out.println("The average is "+(sum/num));
	}

}
